{
  //#if (IsSteeltoeV3)
  "$schema": "https://steeltoe.io/schema/v3/schema.json",
  //#endif
  //#if (ConfigurationPlaceholderOption)
  "ResolvedPlaceholderFromEnvVariables": "${PATH?NotFound}",
  "UnresolvedPlaceholder": "${SomKeyNotFound?NotFound}",
